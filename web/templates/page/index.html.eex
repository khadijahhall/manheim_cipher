<div class="jumbotron">
  <%= form_for @changeset, page_path(@conn, :create), fn f -> %>
<div class="form-group">
  <%=text_input f, :text, placeholder: "Text", class: "form-control" %>
  <%= error_tag f, :text %>
</div>
<div class="form-group">
  <%=text_input f, :shift, placeholder: "Shift Number", class: "form-control" %>
  <%= error_tag f, :shift %>
</div>

<%= submit "Encrypt!", class: "btn btn-primary" %>

<% end%>
</div>
